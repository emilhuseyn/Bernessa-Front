      "CreateOrderDTO": {
        "required": [
          "customerEmail",
          "customerName",
          "customerPhone",
          "items",
          "paymentMethod",
          "shippingAddress"
        ],
        "type": "object",
        "properties": {
          "customerName": {
            "maxLength": 200,
            "minLength": 1,
            "type": "string"
          },
          "customerEmail": {
            "maxLength": 200,
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "customerPhone": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string",
            "format": "tel"
          },
          "shippingAddress": {
            "maxLength": 500,
            "minLength": 1,
            "type": "string"
          },
          "customerNote": {
            "maxLength": 1000,
            "type": "string",
            "nullable": true
          },
          "items": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/OrderItemDTO"
            }
          },
          "paymentMethod": {
            "$ref": "#/components/schemas/PaymentMethod"
          },
          "discountCode": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginDTO": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "OrderItemDTO": {
        "required": [
          "productId",
          "quantity"
        ],
        "type": "object",
        "properties": {
          "productId": {
            "type": "integer",
            "format": "int32"
          },
          "quantity": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "OrderStatus": {
        "enum": [
          "Pending",
          "Processing",
          "Shipped",
          "Delivered",
          "Cancelled"
        ],
        "type": "string",
        "format": "int32"
      },
      "PaymentMethod": {
        "enum": [
          "Cash",
          "Card",
          "BankTransfer"
        ],
        "type": "string",
        "format": "int32"
      },
      "UpdateOrderStatusDTO": {
        "required": [
          "status"
        ],
        "type": "object",
        "properties": {
          "status": {
            "$ref": "#/components/schemas/OrderStatus"
          }
        },
        "additionalProperties": false
      },
      "UpdateProfileDTO": {
        "type": "object",
        "properties": {
